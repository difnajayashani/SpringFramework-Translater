<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
        version="2.5">

    <display-name>Archetype Created Web Application</display-name>

    <listener>
        <listener-class>listener.AppContextListener</listener-class>
    </listener>


    <servlet>
        <servlet-name>servlet1</servlet-name>
        <servlet-class>servelet.MyServlet</servlet-class>
        <load-on-startup>1</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet2</servlet-name>
        <servlet-class>servelet.MyServletTranslate</servlet-class>
        <load-on-startup>2</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet3</servlet-name>
        <servlet-class>servelet.LogoutServlet</servlet-class>
        <load-on-startup>3</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet4</servlet-name>
        <servlet-class>servelet.AddUserServlet</servlet-class>
        <load-on-startup>4</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet5</servlet-name>
        <servlet-class>servelet.CheckAvailability</servlet-class>
        <load-on-startup>5</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet6</servlet-name>
        <servlet-class>servelet.PopulateUserServlet</servlet-class>
        <load-on-startup>6</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet7</servlet-name>
        <servlet-class>servelet.DeleteUserServlet</servlet-class>
        <load-on-startup>7</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet8</servlet-name>
        <servlet-class>servelet.UpdateUserServlet</servlet-class>
        <load-on-startup>8</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet9</servlet-name>
        <servlet-class>servelet.SearchUserServlet</servlet-class>
        <load-on-startup>9</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet10</servlet-name>
        <servlet-class>servelet.TypeAheadServlet</servlet-class>
        <load-on-startup>10</load-on-startup>

    </servlet>

    <servlet>
        <servlet-name>servlet11</servlet-name>
        <servlet-class>servelet.LoadCityServlet</servlet-class>
        <load-on-startup>11</load-on-startup>

    </servlet>


    <servlet-mapping>
        <servlet-name>servlet1</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>servlet2</servlet-name>
        <url-pattern>/MyServletTranslate</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet3</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet4</servlet-name>
        <url-pattern>/AddUserServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet5</servlet-name>
        <url-pattern>/CheckAvailability</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet6</servlet-name>
        <url-pattern>/PopulateUserServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet7</servlet-name>
        <url-pattern>/DeleteUserServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet8</servlet-name>
        <url-pattern>/UpdateUserServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet9</servlet-name>
        <url-pattern>/SearchUserServlet</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>servlet10</servlet-name>
        <url-pattern>/TypeAheadServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet11</servlet-name>
        <url-pattern>/LoadCityServlet</url-pattern>
    </servlet-mapping>


</web-app>
